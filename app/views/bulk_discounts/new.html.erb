<h4>Create New Bulk Discount for <%= @merchant.name %> </h4>

<%= form_with url: merchant_bulk_discounts_path(@merchant), method: :post, local: true do |f| %>
<%= f.label :percent %>
<%= f.number_field :percent %>

<%= f.label :quantity %>
<%= f.number_field :quantity %>

<%= f.submit "SAVE" %>
<% end %>
